mixin socials()
	+b(class='socials')&attributes(attributes)
		+e.UL(class='list')
			+e.LI(class='item')
				+e.A.link._vk.link(href='#' target='_blank') vkontact
					+icon('vk', 46, 46)
			+e.LI(class='item')
				+e.A.link._facebook.link(href='#' target='_blank') github
					+icon('facebook', 40, 42)
			+e.LI(class='item')
				+e.A.link._instagram.link(href='#' target='_blank') instagram
					+icon('instagram', 40, 42)
			+e.LI(class='item')
				+e.A.link._instagram.link(href='#' target='_blank') instagram
					+icon('twitter', 40, 42)
