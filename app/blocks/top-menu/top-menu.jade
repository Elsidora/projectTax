include /blocks/sub-menu/sub-menu
mixin top-menu(items)
	+b.NAV.top-menu#navigation.navigation
		.nav-header
			//- a.nav-brand(href='#') Navigation
			.nav-toggle
		.nav-menus-wrapper
			+e.UL.list.nav-menu
				+e.LI.item
					a(href='tariffs.html') Тарифы
					+sub-menu({'Эконом':'tariff-econom.html',
										'Комфорт':'tariff-comfort.html',
										'Универсал':'tariff-universal.html',
										'Бизнес':'tariff-biznes.html',
										'Минивэн (6 мест)':'tariff-miniven-6.html',
										'Минивэн (8 мест)':'tariff-miniven-8.html',
										'Микроавтобус':'tariff-minibus.html'
										})
				+e.LI.item
					a(href='airport.html') Такси в аэропорт
					ul.nav-dropdown
						li
							a(href='airport-vnukovo.html') Внуково
						li
							a(href='airport-domodedovo.html') Домодедово
							ul.nav-dropdown
								li
									a(href='domodedovo-korolev.html') Королев - Домодедово
								li
									a(href='domodedovo-krasnogorsk.html') Красногорс - Домодедово
								li
									a(href='domodedovo-lubertsi.html') Люберцы - Домодедово
								li
									a(href='domodedovo-ramenskoe.html') Раменское - Домодедово
								li
									a(href='domodedovo-zelenograd.html') Зеленоград - Домодедово
								li
									a(href='domodedovo-odincovo.html') Одинцово - Домодедово
								li
									a(href='domodedovo-mytishchi.html') Мытищи - Домодедово
								li
									a(href='domodedovo-podolsk.html') Подольск - Домодедово
								li
									a(href='domodedovo-hymki.html') Химки - Домодедово
						li
							a(href='airport-sheremetevo.html') Шереметьево
				+e.LI.item
					a(href='stations.html') Такси на вокзал
					+sub-menu({'Белорусский':'belorusskiy.html',
										'Казанский':'kazanskiy.html',
										'Киевский ':'kievskiy.html',
										'Курский':'kurskiy.html',
										'Ленинградский':'leningradskiy.html',
										'Павелецкий':'paveleckiy.html',
										'Рижский':'rizhskiy.html',
										'Савеловский':'savelovskiy.html',
										'Ярославский':'yaroslavskiy.html'
										})
				+e.LI.item
					a(href='#') Такси по округам
				+e.LI.item
					a(href='#') Такси межгород
				+e.LI.item
					a(href='#') Услуги
					+sub-menu({'Прикурить автомобиль':'start-the-car.html',
										'Такси в область':'region-taxi.html'})
